<div class="card-body">
  <div class="table-responsive">
    <table
      class="table table-bordered"
      id="dataTable"
      width="100%"
      cellspacing="0"
    >
      <thead>
        <tr>
          <th>No</th>
          <th>Name Bank</th>
          <th>Nomor Rekening</th>
          <th>Name</th>
          <th>Image</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach((data, index) => { %>
          <tr>
            <td><%= index + 1 %></td>
            <td><%= data.nameBank %></td>
            <td><%= data.noRekening %></td>
            <td><%= data.name %></td>
            <td class="text-center"><img src="http://localhost:3000/<%= data.imageUrl %>" width="100" alt=""></td>
            <td class="text-center">
              <a
                class="btn btn-success btn-edit"
                href="#"
                type="button"
                data-id="<%= data._id %>"
                data-bank="<%= data.nameBank %>"
                data-no="<%= data.noRekening %>"
                data-name="<%= data.name %>"
                data-image="<%= data.imageUrl %>"
              >
                Edit
              </a>
              |
              <a
                class="btn btn-danger btn-delete"
                href="#"
                type="button"
                data-id="<%= data._id %>"
              >
                Delete
              </a>
            </td>
          </tr>
          <% }) %>
      </tbody>
    </table>
  </div>
</div>
